		//sum of even numbers and odd numbers in a array 
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter size of array :");
		int size = sc.nextInt();
		int[] arr = new int[size];
		System.out.println("Enter element of selected size array : ");
		for(int i=0; i<arr.length; i++) {
			arr[i] = sc.nextInt();
		}
		int sumoddvalue = 0;
		for(int i=0; i< arr.length; i++) {
			if(arr[i]%2 != 0) {
				sumoddvalue = sumoddvalue + arr[i];
				System.out.println("Sum of odd value : "+sumoddvalue);
			}
			else {
				continue;
			}
		}
		
		
		
	        
	        
		
